{
    "build_address": {
      "params": [
        "Street",
        "ZipCode",
        "Country: \"United States\""
      ],
      "steps": [
        {
          "concatenate": {
            "return": "${string(Street) + \", \" + string(ZipCode) + \", \" + Country}"
          }
        }
      ]
    }
}
